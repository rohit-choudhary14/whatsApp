<style>
    #view_profile_body{
        background:whitesmoke;
        position: fixed;
        height:0%;
        width: 35%;
        bottom: 0px;
        transition: 0.5s height;
        z-index: 200;
    }
    #view_profile_header{
        background-color: white;
        padding: 1rem;
        box-shadow: 0px 0px 4px 0px rgb(0,0,0,0.4);
    }
    #view_profile_header div{
        display: flex;
        justify-content: center;
        align-items: center;
        margin-top: 1rem;
    }
    #view_profile_notification_settings{
        margin-top: 1rem;
        padding: 1rem;
        box-shadow: 0px 0px 4px 0px rgb(0,0,0,0.4);
    }
    
    #view_profile_notification_settings{
        background-color: white;
    }
    #view_profile_notification_settings div{
        display: flex;
        align-items: center;
        justify-content:start;
        margin-top: 1rem;
    } 
    #view_profile_behaveour_settings{
        background-color: white;
        
    }
    #view_profile_behaveour_settings div{
        margin-top: 1rem;
        padding: 1rem;
        box-shadow: 0px 0px 4px 0px rgb(0,0,0,0.4);
    } 
    @media only screen and (max-width: 1000px) {
        #view_profile_body{
            width:100%;
        }

    }
</style>
