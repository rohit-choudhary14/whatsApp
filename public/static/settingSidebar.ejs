<style>
    #setting_sidebar_main_panel{
        width:100%;
        height:100%;
        position: absolute;
        z-index: 10;
        top:0px;
        left:-5000px;
        bottom:0px;
        transition: 0.5s left;
        
        background: #fff;
    }
   
   #setting_sidebar_main_panel svg{
        color:#8696a0
    }
    #setting_sidebar_main_panel_header{
        filter: saturate(1.5);
        display:flex;
        align-items: center;
        padding:1rem;
        background: #008069;
        position: fixed;
        width:35%;
        transform: translate(0, 0);
        transition: 0.3s background;
        color: black;
        box-shadow: 0px 0px 5px 0px rgb(0,0,0,0.2);
    
    }
    #add_friend_default_icon_parent{
          
           display: none;
           justify-content: space-between;
           align-items: baseline;
           padding: 1rem;
    }
     @media only screen and (max-width: 1000px) {
        #setting_sidebar_main_panel_header{
          width:100%;
        }
        #add_friend_default_icon_parent{
           display:flex;
          
        }


    }
   
   
    
     
    
</style>
<div id="setting_sidebar_main_panel">
